parameters:
    level: 9
    paths:
        - src
        - tests
    ignoreErrors:
        -
            message: '#^Undefined variable: \$this$#'
            path: tests/*
        -
            message: '#^Call to an undefined method Pest\\Expectation\|Pest\\Support\\Extendable::#'
            path: tests/*
        -
            message: '#^Cannot call method throws\(\) on mixed\.$#'
            path: tests/*
